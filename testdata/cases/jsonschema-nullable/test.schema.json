{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$ref": "#/$defs/.tests.Values",
  "$defs": {
    ".tests.MyEnum": {
      "type": "string",
      "enum": [
        "FOO",
        "BAR",
        "BAZ"
      ]
    },
    ".tests.MyMessage": {
      "properties": {
        "val1": {
          "$ref": "#/$defs/.tests.MyMessage.val1"
        },
        "val2": {
          "$ref": "#/$defs/.tests.MyMessage.val2"
        }
      },
      "type": "object"
    },
    ".tests.MyMessage.val1": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    ".tests.MyMessage.val2": {
      "items": {
        "type": "integer"
      },
      "type": "array"
    },
    ".tests.Values": {
      "properties": {
        "messages": {
          "oneOf": [
            {
              "type": "null"
            },
            {
              "$ref": "#/$defs/.tests.Values.messages"
            }
          ]
        },
        "enums": {
          "oneOf": [
            {
              "type": "null"
            },
            {
              "$ref": "#/$defs/.tests.Values.enums"
            }
          ]
        },
        "strings": {
          "oneOf": [
            {
              "type": "null"
            },
            {
              "$ref": "#/$defs/.tests.Values.strings"
            }
          ]
        },
        "ints": {
          "$ref": "#/$defs/.tests.Values.ints"
        }
      },
      "type": "object",
      "required": [
        "messages",
        "enums",
        "strings",
        "ints"
      ]
    },
    ".tests.Values.enums": {
      "$ref": "#/$defs/.tests.MyEnum"
    },
    ".tests.Values.ints": {
      "type": "integer"
    },
    ".tests.Values.messages": {
      "$ref": "#/$defs/.tests.MyMessage"
    },
    ".tests.Values.strings": {
      "type": "string"
    }
  },
  "type": "object"
}